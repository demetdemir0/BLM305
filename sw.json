const CACHE ='https://demetdemir0.github.io/BLM305/Term_Project/index.html'
const FILES = ['https://demetdemir0.github.io/BLM305/Term_Project/index.html']
function installCB(e) {
    e.waitUntil(
      caches.open(CACHE)
      .then(cache => cache.addAll(FILES))
      .catch(console.log)
    )
  }
  self.addEventListener('install', installCB)
  
  function cacheCB(e) { //cache first
    let req = e.request
    e.respondWith(
      caches.match(req)
      .then(r1 => r1 || fetch(req))
      .catch(console.log)
    )
  }
  self.addEventListener('fetch', cacheCB)
  
